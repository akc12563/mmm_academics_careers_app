<div class="page-header">
  <h3>Saved Contact #<%= @saved_contact.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Student</dt>
      <dd><%= @saved_contact.student_id %></dd>

      <dt>User</dt>
      <dd><%= @saved_contact.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/saved_contacts" class="btn btn-primary">
        Back
      </a>
      <a href="/saved_contacts/<%= @saved_contact.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_saved_contact/<%= @saved_contact.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
