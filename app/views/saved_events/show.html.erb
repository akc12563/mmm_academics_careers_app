<div class="page-header">
  <h3>Saved Event #<%= @saved_event.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Event</dt>
      <dd><%= @saved_event.event_id %></dd>

      <dt>User</dt>
      <dd><%= @saved_event.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/saved_events" class="btn btn-primary">
        Back
      </a>
      <a href="/saved_events/<%= @saved_event.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_saved_event/<%= @saved_event.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
